{
    "name": "passhub-api",
    "description": "PassHub API Client",
    "keywords": [
        "PassHub",
        "SightseeingPass",
        "API Client",
        "API"
    ],
    "homepage": "http://sightseeing.tech",
    "authors": [
        {
            "name": "BYM Development LLC",
            "email": "dev@sightseeingtech.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~1.12"
    },
    "autoload": {
        "psr-4": { "passhub\\client\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "passhub\\client\\" : "test/" }
    }
}
